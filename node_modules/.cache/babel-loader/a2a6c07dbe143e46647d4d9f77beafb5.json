{\n  name: \"DynamicTheme\",\n  props: {\n    themes: Object,\n  },\n  methods: {\n    getVars: function (theme) {\n      let vars = [];\n      for (const themeVars in theme) {\n        let value = `${theme[themeVars]}`;\n        if (!value) {\n          value = \"initial\";\n        } else if (themeVars == \"background-image\") {\n          value = `url(${theme[themeVars]})`;\n        }\n        vars.push(`--${themeVars}: ${value}`);\n      }\n      return vars.join(\";\");\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}