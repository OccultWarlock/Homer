;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AADA;AAhBA,CAAA","sourcesContent":["<template>\n  <Generic :item=\"item\">\n    <template #content>\n      <p class=\"title is-4\">{{ item.name }}</p>\n      <p class=\"subtitle is-6\">\n        <template v-if=\"item.subtitle\">\n          {{ item.subtitle }}\n        </template>\n        <template v-else-if=\"meal\"> Today: {{ meal.name }} </template>\n        <template v-else-if=\"stats\">\n          happily keeping {{ stats.totalRecipes }} recipes organized\n        </template>\n      </p>\n    </template>\n  </Generic>\n</template>\n\n<script>\nimport service from \"@/mixins/service.js\";\nimport Generic from \"./Generic.vue\";\n\nexport default {\n  name: \"Mealie\",\n  mixins: [service],\n  props: {\n    item: Object,\n  },\n  components: {\n    Generic,\n  },\n  data: () => ({\n    stats: null,\n    meal: null,\n  }),\n  created() {\n    this.fetchStatus();\n  },\n  methods: {\n    fetchStatus: async function () {\n      const headers = {\n        Authorization: \"Bearer \" + this.item.apikey,\n        Accept: \"application/json\",\n      };\n\n      if (this.item.subtitle != null) return;\n\n      this.meal = await this.fetch(\"/api/meal-plans/today/\", { headers }).catch(\n        (e) => console.log(e)\n      );\n      this.stats = await this.fetch(\"/api/debug/statistics/\", {\n        headers,\n      }).catch((e) => console.log(e));\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/components/services"}]}